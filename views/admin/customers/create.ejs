            <div class="container-fluid  w-100">
                <div>
                    <h1>Create Customer</h1>
                </div>

                <div class="catagoery-form">
                    <form id="create-catagoery-form" method="post">
                        <div class=" row">
                            <div class="col">
                                <label for="profile_image" class="form-label">Select Profile Image</label>
                                <input class="form-control" type="file" name="profile_image" id="formFile">
                            </div>
                        </div>

                        <div class=" row">
                            <div class="col">
                                <label for="firstName" class="form-label lable-text">First Name</label>
                                <input type="text" class="form-control" name="firstName" id="exampleInputEmail1">
                            </div>
                            <div class="col">
                                <label for="lastName" class="form-label lable-text">Last Name</label>
                                <input type="text" class="form-control" name="lastName" id="exampleInputEmail1">
                            </div>
                        </div>
                        <div class="mb-3">
                            <label for="email" class="form-label lable-text">User Email</label>
                            <input type="email" class="form-control" name="email" id="exampleInputEmail1">

                        </div>
                        <div class="mb-3">
                            <label for="password" class="form-label lable-text">password</label>
                            <input type="password" class="form-control" name="password" id="exampleInputEmail1">
                        </div>
                        <div class="mb-3">
                            <label for="userName" class="form-label lable-text">User Name</label>
                            <input type="text" class="form-control" name="userName" id="exampleInputEmail1">
                        </div>
                        <div class="mb-3">
                            <label for="phone" class="form-label lable-text">Phone</label>
                            <input type="tel" class="form-control" name="phone" id="exampleInputEmail1">
                        </div>

                        <div class="btn-contianer">
                            <button type="submit" class="btn button-style">Create Coustomer</button>
                        </div>

                    </form>
                </div>
            </div>

        </section>
